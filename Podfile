platform :ios, '9.0'
use_frameworks!

target 'iTVShows' do
    pod 'SwiftyJSON'
    pod 'CryptoSwift', :git => "https://github.com/krzyzanowskim/CryptoSwift", :branch => "master"
    pod 'Kingfisher'
    pod 'Parse'
    pod 'Alamofire'
    pod 'APESuperHUD', :git => 'https://github.com/apegroup/APESuperHUD.git'
end
